    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="decimal23.4">
    <xs:restriction base="xs:decimal">
      <xs:totalDigits value="23" />
      <xs:fractionDigits value="4" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="numeric4">
    <xs:restriction base="xs:string">
      <xs:maxLength value="4" />
      <xs:pattern value="\d*" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>